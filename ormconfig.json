{
    "type": "postgres",
    "host": "localhost",
    "port": 5432,
    "username": "postgres",
    "entities": "['src/**/*.entity.ts']",
    "database": "hornest",
    "synchronize": true,
    "autoLoadEntities": true,
    "migrations": [
        "migration/*"
    ],
    "cli": {
        "migrationsDir": "migrations"
    },
    "migrationsRun": true
}